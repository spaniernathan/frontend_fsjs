{
    "root": true,
    "plugins": [
        "import",
        "@typescript-eslint"
    ],
    "extends": [
        // "./tsconfig.json",
        "airbnb",
        "airbnb-typescript"
    ],
    "rules": {
        "react/jsx-filename-extension": [2, { "extensions": [".js", ".jsx", ".ts", ".tsx"]}]
    },
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "project": "./tsconfig.json",
        "sourceType": "module"
    }
}
